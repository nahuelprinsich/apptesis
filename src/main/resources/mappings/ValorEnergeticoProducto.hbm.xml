<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.tesis.models">
    <class name="ValorEnergeticoProducto" table="valorenergetico_producto">
        <composite-id name="idValorEnergeticoProducto" class="ValorEnergeticoProductoId">
            <key-many-to-one name="producto" class="Producto" column="Producto_idProducto"/>
            <key-many-to-one name="valorEnergetico" class="ValorEnergetico" column="ValorEnergetico_idValorEnergetico"/>
        </composite-id>
        <property name="valor" column="valor" />
        <property name="tipoPorcion" column="tipoPorcion" />
    </class>
</hibernate-mapping>