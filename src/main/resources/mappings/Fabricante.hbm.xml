<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.tesis.models">
    <class name="Fabricante" table="fabricante">
        <id name="idFabricante" type="integer" column="idFabricante" unsaved-value="0">
            <generator class="native" ></generator>
        </id>
        <property name="domicilio" column="domicilio" />
        <property name="razonSocial" column="razonSocial" />
        <property name="pais" column="pais" />
        <property name="localidad" column="localidad" />
    </class>
</hibernate-mapping>