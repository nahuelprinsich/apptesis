<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.tesis.models">
    <class name="ValorEnergetico" table="valorenergetico">
        <id name="idValorEnergetico"  column="idValorEnergetico" unsaved-value="0" type="integer">
            <generator class="native" ></generator>
        </id>
        <property name="nombre" column="nombre" />
        <property name="descripcion" column="descripcion" />
        <property name="recomendableDiario" column="recomendableDiario" />
        <property name="tipoPorcion" column="tipoPorcion" />
        <property name="linkInformacionExtra" column="linkInformacionExtra" />
    </class>
</hibernate-mapping>