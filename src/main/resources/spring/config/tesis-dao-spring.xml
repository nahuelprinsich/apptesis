<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:mvc="http://www.springframework.org/schema/mvc" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
		http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
		http://www.springframework.org/schema/context
		http://www.springframework.org/schema/context/spring-context-4.0.xsd
		http://www.springframework.org/schema/mvc
		http://www.springframework.org/schema/mvc/spring-mvc-4.0.xsd">

    <!-- Data Access Object -->

    <bean id="comentario.hibernate.dao" class="com.tesis.dao.impl.ComentarioDAOImpl" >
        <property name="sessionFactory" ref="sessionFactory"></property>
    </bean>

    <bean id="item.hibernate.dao" class="com.tesis.dao.impl.ItemDAOImpl" >
        <property name="sessionFactory" ref="sessionFactory"></property>
    </bean>

    <bean id="producto.hibernate.dao" class="com.tesis.dao.impl.ProductoDAOImpl" >
        <property name="sessionFactory" ref="sessionFactory"></property>
    </bean>

    <bean id="envase.hibernate.dao" class="com.tesis.dao.impl.EnvaseDAOImpl" >
        <property name="sessionFactory" ref="sessionFactory"></property>
    </bean>

    <bean id="extra.hibernate.dao" class="com.tesis.dao.impl.ExtraDAOImpl" >
        <property name="sessionFactory" ref="sessionFactory"></property>
    </bean>

    <bean id="fabricante.hibernate.dao" class="com.tesis.dao.impl.FabricanteDAOImpl" >
        <property name="sessionFactory" ref="sessionFactory"></property>
    </bean>

    <bean id="ingrediente.hibernate.dao" class="com.tesis.dao.impl.IngredienteDAOImpl" >
        <property name="sessionFactory" ref="sessionFactory"></property>
    </bean>

    <bean id="valorEnergetico.hibernate.dao" class="com.tesis.dao.impl.ValorEnergeticoDAOImpl" >
        <property name="sessionFactory" ref="sessionFactory"></property>
    </bean>

</beans>